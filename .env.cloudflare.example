# OpenRouter
OPEN_ROUTER_API_KEY=

# ======================================
# CUSTOM PROVIDER BASE URLS
# ======================================

# Ollama (must be publicly accessible URL for Cloudflare)
OLLAMA_API_BASE_URL=

# OpenAI-like API
OPENAI_LIKE_API_BASE_URL=
OPENAI_LIKE_API_KEY=

# Together AI Base URL
TOGETHER_API_BASE_URL=

# Hyperbolic Base URL
HYPERBOLIC_API_BASE_URL=

# LMStudio (must be publicly accessible URL for Cloudflare)
LMSTUDIO_API_BASE_URL=

# ======================================
# CLOUD SERVICES CONFIGURATION
# ======================================

# AWS Bedrock Configuration (JSON format)
AWS_BEDROCK_CONFIG=

# ======================================
# SERVICE INTEGRATIONS
# ======================================

# GitHub Personal Access Token
VITE_GITHUB_ACCESS_TOKEN=
VITE_GITHUB_TOKEN_TYPE=classic

# GitLab Personal Access Token
VITE_GITLAB_ACCESS_TOKEN=
VITE_GITLAB_URL=https://gitlab.com
VITE_GITLAB_TOKEN_TYPE=personal-access-token

# Vercel Access Token
VITE_VERCEL_ACCESS_TOKEN=

# Netlify Access Token
VITE_NETLIFY_ACCESS_TOKEN=

# Supabase Configuration
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=
VITE_SUPABASE_ACCESS_TOKEN=

# ======================================
# PRODUCTION SETTINGS
# ======================================

# Environment
NODE_ENV=production

# Logging Level
VITE_LOG_LEVEL=info

# Default Context Window Size
DEFAULT_NUM_CTX=32768

# ======================================
# INSTRUCTIONS FOR CLOUDFLARE PAGES
# ======================================
# 1. Go to your Cloudflare Pages project dashboard
# 2. Navigate to Settings > Environment Variables
# 3. Add each variable you need from above
# 4. Set them for "Production" and/or "Preview" environments
# 5. Redeploy your project after setting variables
# 6. Variables starting with VITE_ are exposed to the client
# 7. Other variables are only available server-side
